{
  "name": "noir-starter",
  "version": "1.0.0",
  "type": "module",
  "description": "A template repository to get started with writing zero knowledge programs with Noir.",
  "scripts": {
    "dev:site": "pnpm run build:site && vite",
    "dev:contracts": "hardhat compile && pnpm run build:contracts",
    "dev": "dev:site & dev:contracts",
    "build:site": "vite build",
    "build:contracts": "hardhat compile && pnpm run deploy",
    "build": "pnpm run build:site && pnpm run build:contracts",
    "preview": "vite preview",
    "deploy": "hardhat compile && NODE_OPTIONS='--experimental-loader ts-node/esm/transpile-only' hardhat run --network ${NETWORK} scripts/deploy.ts",
    "test": "NODE_OPTIONS='--experimental-loader ts-node/esm/transpile-only' hardhat test"
  },
  "dependencies": {
    "@noir-lang/acvm_js": "^0.38.0",
    "@noir-lang/backend_barretenberg": "^0.19.4",
    "@noir-lang/noir_js": "^0.19.4",
    "@noir-lang/noir_wasm": "^0.19.4",
    "@noir-lang/noirc_abi": "^0.22.0",
    "@noir-lang/source-resolver": "^0.19.4",
    "@noir-lang/types": "^0.19.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox-viem": "1.0.0",
    "@nomicfoundation/hardhat-viem": "1.0.0",
    "dotenv": "^16.0.3",
    "hardhat": "^2.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^9.1.1",
    "typechain": "^8.1.0",
    "viem": "1.16.6"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.26",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "chai": "^4.2.0",
    "daisyui": "^4.6.0",
    "hardhat-gas-reporter": "^1.0.9",
    "postcss": "^8.4.33",
    "solidity-coverage": "^0.8.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^5.0.6",
    "wagmi": "^1.4.8"
  },
  "engines": {
    "node": "^20.10.0"
  }
}
